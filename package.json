{
	"name": "hubot-eve-tripwire",
	"version": "0.0.1",
	"description": "A Hubot script that integrates with Tripwire",
	"main": "index.js",
	"scripts": {
		"develop": "babel src -d build --watch",
		"build": "babel src -d build",
		"test": "mocha || true",
		"coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage && istanbul cover node_modules/mocha/bin/_mocha"
	},
	"keywords": [
		"tripwire",
		"eve",
		"online",
		"eve",
		"hubot"
	],
	"author": "George Sumpster",
	"license": "MIT",
	"peerDependencies": {
		"hubot": "2.x"
	},
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-preset-latest": "^6.16.0",
		"chai": "^3.5.0",
		"eslint": "^3.11.1",
		"eslint-config-google": "^0.7.1",
		"eslint-plugin-mocha": "^4.7.0",
		"mocha": "^3.2.0"
	},
	"dependencies": {
		"codacy-coverage": "^2.0.0",
		"mocha-lcov-reporter": "^1.2.0",
		"node-dijkstra": "^2.3.0",
		"request": "^2.79.0",
		"request-promise": "^4.1.1",
		"request-promise-native": "^1.0.3"
	}
}
